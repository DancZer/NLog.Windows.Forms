<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="false"
      internalLogLevel="Debug" internalLogFile="c:\temp\nlog-window.log" >


  <targets>
    <target xsi:type="RichTextBox"
            name="target1"
            layout="${message}"
         
            autoScroll="true"
            maxLines="10"
     
          
      
 
         
            useDefaultRowColoringRules="true" />

  
            
            
           
      <!--<word-coloring backgroundColor="String"
                   fontColor="String" ignoreCase="Boolean"
                   regex="String"
                   style="Enum"
                   text="String"
                   wholeWords="Boolean"/>


    <row-coloring backgroundColor="String"
                  condition="Condition"
                 fontColor="String"
                 style="Enum"/>-->
  </targets>
  <rules>

    <logger name="*" minlevel="Debug" writeTo="target1" />

  </rules>
</nlog>